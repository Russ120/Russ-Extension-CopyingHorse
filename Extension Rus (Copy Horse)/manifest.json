
{
  "manifest_version": 3,
  "name": "Russ Extension",
  "version": "1.0",
  "description": "Una extensi√≥n para leer texto seleccionado con voz",
  "permissions": ["activeTab", "storage", "clipboardRead","contextMenus"],
  "action": {
    "default_popup": "popup.html",
    "icon":"icon2.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ]
}
